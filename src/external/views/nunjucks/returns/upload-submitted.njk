{% extends "./nunjucks/layout.njk" %}
{% from "govuk/components/panel/macro.njk" import govukPanel %}

{% block content %}

<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">

    {{ govukPanel({
      titleText: pageTitle
    }) }}

    {% if xmlUser %}
      <p>
        <a href="/returns/upload">Upload more returns</a>
      </p>
    {% endif %}

    <p>
      <a href="/returns">Returns overview</a>
    </p>
  </div>
</div>

{% endblock %}
