{% extends "./nunjucks/layout.njk" %}
{% from "nunjucks/macros/forms/index.njk" import formRender, formErrorSummary %}
{% from "nunjucks/macros/title.njk" import title %}

{% block content %}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-full">
    {{ formErrorSummary(form) }}
    {{ title(pageTitle) }}
    {{ formRender(form) }}
    </div>
  </div>
{% endblock %}
